<% for( let i=0; i < users.length; i++ ) { %>

     <tr>
          <td>
               <%= i+1 %>
          </td>
          <td>
               <%= users[i].name %>
          </td>
          <td>
               <%= users[i].email %>
          </td>
          <td>
               <%= users[i].gender %>
          </td>
          <td>
               <%= users[i].status %>
          </td>
          <td>
               <a href="/update-user?id=<%= users[i]._id %> " class="btn border-shadow update">
                    <i class="fa-solid fa-pencil"></i>
               </a>
               <a data-id="<%= users[i]._id %> " class="btn border-shadow delete">
                    <i class="fa-solid fa-trash"></i>
               </a>
          </td>
     </tr>
     <% } %>